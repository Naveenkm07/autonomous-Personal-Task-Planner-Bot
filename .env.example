
# API Keys and Credentials
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_CALENDAR_CREDENTIALS=path_to_credentials.json
NOTION_API_TOKEN=your_notion_integration_token
OPENWEATHER_API_KEY=your_openweather_api_key
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Database Configuration
DB_PASSWORD=secure_database_password

# Monitoring
GRAFANA_PASSWORD=admin_password_here

# System Configuration
ENVIRONMENT=production
LOG_LEVEL=INFO
TIMEZONE=UTC

# Agent Configuration
COLLECTOR_SCHEDULE=*/15 * * * *
REVIEWER_SCHEDULE=0 22 * * *
MAX_RETRIES=3
TIMEOUT=30

# Rate Limiting
GOOGLE_API_RATE_LIMIT=100
NOTION_API_RATE_LIMIT=120
OPENWEATHER_API_RATE_LIMIT=60
TELEGRAM_API_RATE_LIMIT=30
GEMINI_API_RATE_LIMIT=300
